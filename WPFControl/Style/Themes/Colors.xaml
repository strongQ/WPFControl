<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--重点人员预警-->

    <!--默认组件边框颜色-->
    <SolidColorBrush x:Key="UnfocusedDefaultControlBorderColor" Color="#009BFF"/>
    <SolidColorBrush x:Key="DefaultControlBorderColor" Color="#009BFF"/>
    <!--默认前景色-->
    
    <SolidColorBrush x:Key="DefaultForeground" Color="White"/>

    <SolidColorBrush x:Key="DefaultHeaderForeground" Color="#009BFF"/>
    <SolidColorBrush x:Key="DefaultHeaderHighlightForeground" Color="#27DEED"/>
    <SolidColorBrush x:Key="DefaultHighlightForeground" Color="#27DEED"/>
    
    <SolidColorBrush x:Key="DefaultForegroundsOnSelection" Color="#008CFF"/>


    <!--文本-->
    <SolidColorBrush x:Key="ContentText" Color="#0863a1" Opacity="0.9"/>
    <!--容器边框-->
    <SolidColorBrush x:Key="xiaqujingli_list_boder" Color="#0A1521"/>
    <!--容器背景-->
    <SolidColorBrush x:Key="xiaqujingli_list_backgroud" Color="#000000" Opacity="0.3"/>
    <!--容器抬头-->
    <SolidColorBrush x:Key="ListBoxHeaderText" Color="#099FD6"/>
</ResourceDictionary>